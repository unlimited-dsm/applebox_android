<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.dsm.unlimited.applebox_android.ui.fragment.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_politicsWritePosterFragment"
            app:destination="@id/politicsWritePosterFragment" />
        <action
            android:id="@+id/action_mainFragment_to_politicsReadPosterFragment"
            app:destination="@id/politicsReadPosterFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_politics_write_poster"
        android:id="@+id/politicsWritePosterFragment"
        android:name="com.dsm.unlimited.applebox_android.ui.fragment.PoliticsWritePosterFragment"
        android:label="PoliticsWritePosterFragment" />
    <fragment
        tools:layout="@layout/fragment_politics_read_poster"
        android:id="@+id/politicsReadPosterFragment"
        android:name="com.dsm.unlimited.applebox_android.ui.fragment.PoliticsReadPosterFragment"
        android:label="PoliticsReadPosterFragment" />
</navigation>