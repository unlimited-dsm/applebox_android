<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProfileView">
        <attr name="image" format="reference|string"/>
        <attr name="name" format="reference|string"/>
        <attr name="subName" format="reference|string"/>
        <attr name="introduce" format="reference|string"/>
    </declare-styleable>

    <declare-styleable name="CustomProfileView">
        <attr name="icon" format="reference"/>
        <attr name="iconColor" format="reference"/>
        <attr name="badgeText" format="integer|reference"/>
        <attr name="badgeColor" format="reference"/>
        <attr name="badgeTextColor" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="backGroundImage" format="string|reference"/>
    </declare-styleable>

    <declare-styleable name="SelectFileFAB">
        <attr name="fabSize"/>
        <attr name="fabImage1"/>
        <attr name="fabImage2"/>
        <attr name="fabImage3"/>
    </declare-styleable>
</resources>